entity = {
	name = "elven_01_SCX_Carrier_entity"
	pdxmesh = "elven_01_capital_frame_mesh"
	
	default_state = "idle"
	state = { name = "idle"	animation = "idle" animation_blend_time = 4.0 
		start_event = { trigger_once = yes sound = { soundeffect = "amb_battleship_hum" } }
		start_event = { trigger_once = yes sound = { soundeffect = "amb_battleship_bass" } }
	}
	state = { name = "moving"	animation = "idle" animation_blend_time = 4.0 
		event = { trigger_once = yes sound = { soundeffect = "moving_out_battleship" } }
	}
	state = { name = "death" animation = "death" animation_blend_time = 0.0 looping = no
		
		event = { time = 15.75 node = "ship_main" particle = "elven_01_supercapital_explosion_particle" sound = { soundeffect = ship_destroyed } }
	}
	
	scale = @frame_scale
		
	game_data = {
		size = @frame_size
	}
}

